REGCLIENT=$( curl -s -X POST https://eu-mexdemo.dme.mobiledgex.net:38001/v1/registerclient \
        -d '{"app_name": "MobiledgeX SDK Demo", "app_vers": "2.0", "dev_name": "MobiledgeX"}' )

SESSCOOKIE=$( echo "$REGCLIENT" | jq -r .session_cookie )

curl -s -X POST https://eu-mexdemo.dme.mobiledgex.net:38001/v1/findcloudlet \
        -d '{
                "session_cookie": "'${SESSCOOKIE}'",
                "carrier_name": "TDG",
                "gps_location": {"latitude": 48.0, "longitude": 11.0},
                "ver": 1
        }'
