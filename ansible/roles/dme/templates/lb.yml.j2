apiVersion: v1
kind: Service
metadata:
  labels:
    app: dme
  name: dme
  namespace: default
spec:
  externalTrafficPolicy: Cluster
  ports:
   - name: http
     port: 38001
     protocol: TCP
     targetPort: 38001
   - name: grpc
     port: 50051
     protocol: TCP
     targetPort: 50051
  selector:
    app: dme
  sessionAffinity: None
  type: LoadBalancer
